/* Program to reverse the elements of an array */

#include <stdio.h>
int main()
{
  int i, j, temp, arr[5] = {1, 2, 3, 4, 5};
  for(i=0, j=4; i<j; i++, j--)
  {
    temp = arr[i];
    arr[i] = arr[j];
    arr[j] = temp;
  }
  printf("After reversing the array is : ");
  for(i=0; i<5; i++)
    printf("%d ", arr[i]);
  printf("\n");
}
